<p id="notice"><%= notice %></p>

<h1>Listing Seo Pages</h1>

<table>
  <thead>
    <tr>
      <th>H1 text</th>
      <th>H2 text</th>
      <th>Og description</th>
      <th>Slug</th>
      <th>Image tag</th>
      <th>Name</th>
      <th>Paragraph text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seo_pages.each do |seo_page| %>
      <tr>
        <td><%= seo_page.h1_text %></td>
        <td><%= seo_page.h2_text %></td>
        <td><%= seo_page.og_description %></td>
        <td><%= seo_page.slug %></td>
        <td><%= seo_page.image_tag %></td>
        <td><%= seo_page.name %></td>
        <td><%= seo_page.paragraph_text %></td>
        <td><%= link_to 'Show', seo_page %></td>
        <td><%= link_to 'Edit', edit_seo_page_path(seo_page) %></td>
        <td><%= link_to 'Destroy', seo_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seo page', new_seo_page_path %>
